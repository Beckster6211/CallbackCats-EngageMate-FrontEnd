Change} />\r\n      </FormControl>\r\n      <Button colorScheme=\"teal\" type=\"submit\">\r\n        Send\r\n      </Button>\r\n    </form>\r\n  );\r\n}\r\n\r\nexport default MassAlert;\r\n","C:\\Users\\becky\\Documents\\Coding\\SoC\\SchoolWork\\projectMonth\\callbackCats\\front-end-final-project-callback-cats\\thumb-o-meter\\src\\pages\\back-home\\index.js",[],"C:\\Users\\becky\\Documents\\Coding\\SoC\\SchoolWork\\projectMonth\\callbackCats\\front-end-final-project-callback-cats\\thumb-o-meter\\src\\components\\spotify\\index.js",["270","271","272","273","274"],"C:\\Users\\becky\\Documents\\Coding\\SoC\\SchoolWork\\projectMonth\\callbackCats\\front-end-final-project-callback-cats\\thumb-o-meter\\src\\components\\sessionTable\\index.js",["275","276"],"C:\\Users\\becky\\Documents\\Coding\\SoC\\SchoolWork\\projectMonth\\callbackCats\\front-end-final-project-callback-cats\\thumb-o-meter\\src\\components\\userTable\\index.js",["277"],"C:\\Users\\becky\\Documents\\Coding\\SoC\\SchoolWork\\projectMonth\\callbackCats\\front-end-final-project-callback-cats\\thumb-o-meter\\src\\components\\timer\\index.js",[],"C:\\Users\\becky\\Documents\\Coding\\SoC\\SchoolWork\\projectMonth\\callbackCats\\front-end-final-project-callback-cats\\thumb-o-meter\\src\\components\\addUserForm\\index.js",[],"C:\\Users\\becky\\Documents\\Coding\\SoC\\SchoolWork\\projectMonth\\callbackCats\\front-end-final-project-callback-cats\\thumb-o-meter\\src\\components\\massAlert\\startSession.js",["278"],"import React, { useState } from \"react\";\r\nimport styles from \"./index.module.css\";\r\nimport { Button } from \"@chakra-ui/react\";\r\nimport useSocketContext from \"../../context/socketContext\";\r\n\r\nfunction StartSession({ message, icon }) {\r\n  const context = useSocketContext();\r\n  const socket = context[0];\r\n\r\n  function handleSubmit(e) {\r\n    e.preventDefault();\r\n    console.log({ message });\r\n    try {\r\n      socket.emit(\"massMessage\", { message });\r\n    } catch (err) {\r\