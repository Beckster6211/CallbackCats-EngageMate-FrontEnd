r} bg={bg} color={color}>\r\n      <h1>{data.question}</h1>\r\n      <Thumb value={value} />\r\n      <Slider\r\n        aria-label=\"slider-ex-1\"\r\n        defaultValue={30}\r\n        onChangeEnd={(val) => setValue(val)}\r\n        isDisabled={count > 0 ? false : true}\r\n      >\r\n        <SliderTrack>\r\n          <SliderFilledTrack />\r\n        </SliderTrack>\r\n        <SliderThumb />\r\n      </Slider>\r\n      <h3>Value: {value}%</h3>\r\n      <p>\r\n        {data.responses}/{data.participants} {<Icon as={MdPeople} />}\r\n      </p>\r\n      <p>{count}</p>\r\n      <Timer count={count} time={time} />\r\n    </div>\r\n  );\r\n}\r\n\r\nexport default PtView;\r\n","/Users/jwilsonleigh/Desktop/soc/final-project/front-end-final-project-callback-cats/thumb-o-meter/src/components/skView/index.js",[],"/Users/jwilsonleigh/Desktop/soc/final-project/front-end-final-project-callback-cats/thumb-o-meter/src/components/timer/index.js",[],"/Users/jwilsonleigh/Desktop/soc/final-project/front-end-final-project-callback-cats/thumb-o-meter/src/components/logout/index.js",[],"/Users/jwilsonleigh/Desktop/soc/final-project/front-end-final-project-callback-cats/thumb-o-meter/src/components/thumb/index.js",[],"/Users/jwilsonleigh/Desktop/soc/final-project/front-end-final-project-callback-cats/thumb-o-meter/src/components/featureIcons/index.js",[],"/Users/jwilsonleigh/Desktop/soc/final-project/front-end-final-project-callback-cats/thumb-o-meter/src/pages/raise-hand/index.js",[],"/Users/jwilsonleigh/Desktop/soc/final-project/front-end-final-project-callback-cats/thumb-o-meter/src/pages/quiz/index.js",[],"/Users/jwilsonleigh/Desktop/soc/final-project/front-end-final-project-callback-cats/thumb-o-meter/src/pages/dj-deck/index.js",[],"/Users/jwilsonleigh/Desktop/soc/final-project/front-end-final-project-callback-cats/thumb-o-meter/src/components/menu/index.js",[],"/Users/jwilsonleigh/Desktop/soc/final-project/front-end-final-project-callback-cats/thumb-o-meter/src/components/theme/in