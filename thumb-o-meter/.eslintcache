\n\r\nexport default Header;\r\n","C:\\Users\\becky\\Documents\\Coding\\SoC\\SchoolWork\\projectMonth\\callbackCats\\front-end-final-project-callback-cats\\thumb-o-meter\\src\\components\\menu\\index.js",["239"],"import React, { useEffect } from \"react\";\r\nimport FeatureIcon from \"../featureIcons\";\r\nimport styles from \"./menu.module.css\";\r\nimport useRoleContext from \"../../context/roleContext\";\r\nimport useSocketContext from \"../../context/socketContext\";\r\nimport HandNotify from \"../notify\";\r\nimport { features, coachFeatures } from \"./data\";\r\nimport { Box } from \"@chakra-ui/react\";\r\n\r\nconst Menu = ({ bg, color }) => {\r\n  const result = useRoleContext();\r\n  const role = result[0];\r\n  const context = useSocketContext();\r\n  const socket = context[0];\r\n  const loggedUser = result[2];\r\n  const name = loggedUser?.given_name;\r\n  useEffect(() => {\r\n    socket.emit(\"mainmenuroom\", {\r\n      name: name,\r\n      room: \"mainmenu\",\r\n    });\r\n  }, []);\r\n  return (\r\n    <>\r\n      <div className={styles.container}>\r\n        <div className={styles.players}>\r\n          {role === \"coach\"\r\n            ? coachFeatures.map((item) => {\r\n                return (\r\n                  <section key={item.heading}>\r\n                    <FeatureIcon\r\n                      alt={item.alt}\r\n                      src={item.src}\r\n                      link={item.link}\r\n                      icon={item.icon}\r\n                      heading={item.heading}\r\n                      myClass={item.myClass}\r\n                      id={item.id}\r\n                      notify={<HandNotify />}\r\n                    />\r\n                  </section>\r\n                );\r\n              })\r\n            : features.map((item) => {\r\n                return (\r\n                  <section key={item.heading}>\r\n                    <FeatureIcon\r\n                      alt={item.alt}\r\n                      src={item.src}\